void setup()
{
  pinMode(sens1, INPUT);
  pinMode(sens2, INPUT);
  Serial.begin(9600);
  Wire.begin(D2, D1);
  u8g2.setI2CAddress(0x3C * 2);
  u8g2.begin();
  u8g2_2.setI2CAddress(0x3D * 2);
  u8g2_2.begin();
  WiFi.mode(WIFI_STA);
  WiFi.disconnect();

  if (esp_now_init() != 0)
  {
    Serial.println("ESP-NOW Init Failed");
    return;
  }
  esp_now_set_self_role(ESP_NOW_ROLE_CONTROLLER); // MASTER
  esp_now_register_send_cb(OnDataSent);
  esp_now_add_peer(car1, ESP_NOW_ROLE_SLAVE, 1, NULL, 0);
  esp_now_add_peer(car2, ESP_NOW_ROLE_SLAVE, 1, NULL, 0);
  Gate.attach(D5, 500, 2500);
  Gate.write(open_angle);
  delay(1000);
}
void loop()
{

  counter = detectObject(sens1, true, inc_S1, counter);
  counter = detectObject(sens2, false, inc_S2, counter);
  reciv();
  bool isUsbConnected = (connected == "yes");
  static unsigned long lastTime = 0;
  if (millis() - lastTime > 100)
  {
    lastTime = millis();
    send_tocar();
  }
  if (counter >= max_cars && Deticted != "background")
  {
    draw(isUsbConnected, true);
    draw2(true);
    speed = max_speed;
    Gate.write(close_angle);
  }
  else
  {
    draw(isUsbConnected, false);
    draw2(false);
    speed = normal_speed;
    Gate.write(open_angle);
  }
}

