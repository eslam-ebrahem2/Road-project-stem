  // Layer 3
  u8g2.setDrawColor(2);
  u8g2.setFont(u8g2_font_6x13_tr);
  u8g2.drawStr(7, 19, Layer_3_text);

  // Layer 4
  u8g2.setDrawColor(1);
  u8g2.drawFrame(3, 24, 122, 36);

  // Layer 5
  u8g2.drawLine(64, 25, 64, 58);

  // microphone
  if (connect)
  {
    u8g2.drawXBMP(110, 6, 15, 16, image_microphone_bits);
  }

  // Layer 11

  u8g2.drawStr(98, 56, "10km");

  // Layer 12
  u8g2.drawStr(45, 56, "6km");

  //  image !road >>> close
  if (road_maxmum)
  {
    u8g2.drawXBMP(19, 28, 16, 16, image_download_1_bits);
    // Layer 9
    u8g2.setFont(u8g2_font_6x10_tr);
    u8g2.drawStr(9, 55, "Close");
  }
  else
  {
    // road_with_broken_line__1_
    u8g2.drawXBMP(20, 26, 30, 22, image_road_with_broken_line__1__bits);
    // Layer 9
    u8g2.setFont(u8g2_font_6x10_tr);
    u8g2.drawStr(9, 55, "Open");
  }

  // Layer 13
  u8g2.drawFrame(64, 3, 46, 21);

  // Layer 15
  u8g2.setFont(u8g2_font_6x13_tr);
  u8g2.drawStr(92, 19, String(counter).c_str());

  // download
  u8g2.drawXBMP(68, 3, 19, 16, image_download_2_bits);

  // Layer 17
  u8g2.drawLine(107, 20, 67, 20);

  // fastest
  u8g2.drawXBMP(69, 27, 32, 32, image_fastest_bits);

  u8g2.sendBuffer();
}

void draw2(bool isMaxCars)
{
  if (!isMaxCars)
  {
    u8g2_2.clearBuffer();
    u8g2_2.setFontMode(1);
    u8g2_2.setBitmapMode(1);
    // public_transport
    u8g2_2.drawXBMP(62, -2, 64, 64, image_public_transport_bits);

    // road
    u8g2_2.drawXBMP(0, 1, 64, 64, image_road_bits);

    u8g2_2.sendBuffer();
  }
  //------------------------case 2-----------------------
  else
  {
    u8g2_2.clearBuffer();
    u8g2_2.setFontMode(1);
    u8g2_2.setBitmapMode(1);
    // road_with_broken_line__1_
    u8g2_2.drawXBMP(0, 0, 64, 64, image_road_with_broken_line__2__bits);

    // ambulance
    u8g2_2.setDrawColor(2);
    u8g2_2.drawXBMP(18, 24, 32, 31, image_ambulance_bits);

    // fire_brigade
    u8g2_2.setDrawColor(1);
    u8g2_2.drawXBMP(61, 11, 64, 45, image_fire_brigade_bits);

    u8g2_2.sendBuffer();
  }
}
void send_tocar()
{
  msg.car_speed = speed;
  msg.safe_distance = space;
  esp_now_send(car1, (uint8_t *)&msg, sizeof(msg));
  esp_now_send(car2, (uint8_t *)&msg, sizeof(msg));
}

void reciv()
{
  if (Serial.available())
  {
    String input = Serial.readStringUntil('\n');

    JsonDocument doc;
    DeserializationError error = deserializeJson(doc, input);

    if (error)
    {
      Serial.print("deserializeJson() failed: ");
      Serial.println(error.c_str());
      return;
    }

    const char *detict = doc["detict"];
    const char *conect = doc["connect"];
    Deticted = String(detict);
    connected = String(conect);
  }
}

int detectObject(int pin, bool increment,
                 bool &flag, int &counter)
{

  int v = digitalRead(pin);

  if (v == LOW && !flag)
  {

    if (increment)
      counter++;
    else
      counter--;

    flag = true;
  }

  if (v == HIGH && flag)
  {
    flag = false;
  }

  return counter;
}
